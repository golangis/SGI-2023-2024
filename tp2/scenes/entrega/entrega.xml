
<?xml-model href="https://schema.toino.pt/yaf.xsd" type="application/xml"?>

<yaf>

    <globals background="0 0 0 1" ambient="0.1 0.1 0.1 1" />

    <fog color="1.0 1.0 1.0 1.0" near="1000.0" far="10000.0" />

    <skybox
        size="100 100 100"
        center="0 49.9 0"
        emissive="0 0 0 1"
        intensity="5"
        front="scenes/entrega/texturas-entrega/OPA.jpg"
        back="scenes/entrega/texturas-entrega/OPA.jpg"
        up="scenes/entrega/texturas-entrega/OPA.jpg"
        down="scenes/entrega/texturas-entrega/OPA.jpg"
        left="scenes/entrega/texturas-entrega/OPA.jpg"
        right="scenes/entrega/texturas-entrega/OPA.jpg" />

    <cameras initial="cam1">
        <perspective id="cam1"
            angle="1.047"
            near="0.1"
            far="1000"
            location="15 15 15"
            target="0 0.0 0" />

        <orthogonal id="cam2"
            near="0.1"
            far="1000"
            location="0 15 15"
            target="0.0 15 0.0"
            left="-5"
            right="5"
            bottom="-5"
            top="5" />
    </cameras>

    <!-- bloco de texturas. Este bloco tem de existir mas pode estar vazio, se nao forem usadas
    texturas -->
    <textures>
        <texture id="ceiling-texture" filepath="scenes/entrega/texturas-entrega/ceiling.jpg" />
        <texture id="tile-floor" filepath="scenes/entrega/texturas-entrega/tile-floor.jpg" />
        <texture id="wall" filepath="scenes/entrega/texturas-entrega/wall.png" mipmap0="scenes/entrega/texturas-entrega/wall-half.png" mipmap1="cenes/entrega/texturas-entrega/wall-quarter.png"/>
        <texture id="door-texture" filepath="scenes/entrega/texturas-entrega/door.jpg" />
        <texture id="mirror-texture" filepath="scenes/entrega/texturas-entrega/mirror1.jpg" />
        <texture id="pic1" filepath="scenes/entrega/texturas-entrega/print1.png" />
        <texture id="pic2" filepath="scenes/entrega/texturas-entrega/print2.png" />
        <texture id="basket-texture" filepath="scenes/entrega/texturas-entrega/basket.jpg" />
        <texture id="towels-texture" filepath="scenes/entrega/texturas-entrega/towels.jpg" />
        <texture id="carpet-texture" filepath="scenes\entrega\texturas-entrega\carpet.jpg" />
        <texture id="carpet-texture-bump"
            filepath="scenes\entrega\texturas-entrega\carpet-bump.png" />
        <texture id="wood-texture" filepath="scenes\entrega\texturas-entrega\wood.jpg" />
        <texture id="towel-front-texture" filepath="scenes\entrega\texturas-entrega\towel-front.png" />
        <texture id="video-bath" filepath="scenes/entrega/texturas-entrega/videos/water.mp4"
            isVideo="true" />
        <texture id="shampoo-label-texture-1"
            filepath="scenes\entrega\texturas-entrega\shampoo-label-1.jpg" />
        <texture id="shampoo-label-texture-2"
            filepath="scenes\entrega\texturas-entrega\shampoo-label-2.jpg" />
        <texture id="far-texture" filepath="scenes\entrega\texturas-entrega\far-text.jpg" />
        <texture id="ikea-texture" filepath="scenes\entrega\texturas-entrega\ikea-logo.jpg" />
        <texture id="garage-texture" filepath="scenes\entrega\texturas-entrega\garage-door.png" />

    </textures>

    <materials>

        <material id="garageMaterial"
            textureref="garage-texture"
            color="1 1 1 1"
            specular="0 0 0 1"
            shininess="10"
            emissive="0 0 0 1"
        />

        <material id="ikeaMaterial"
            textureref="ikea-texture"
            color="1 1 1 1"
            specular="0 0 0 1"
            shininess="10"
            emissive="0 0 0 1"
            twosided="true"
        />

        <material id="farMaterial"
            textureref="far-texture"
            color="1 1 1 1"
            specular="0 0 0 1"
            shininess="10"
            emissive="0 0 0 1"
        />


        <material id="white"
            color="1 1 1 1"
            specular="0.7 0.7 0.7 1"
            shininess="10"
            emissive="0 0 0 1"
            twosided="true"
        />

        <material id="black"
            color="0 0 0 1"
            specular="0.4 0.4 0.4 1"
            shininess="5"
            emissive="0 0 0 1"
            twosided="true"
        />

        <material id="grey-metal"
            color="0.5 0.5 0.5 1"
            specular="0.5 0.5 0.5 1"
            shininess="350"
            emissive="0 0 0 0"
            twosided="true"
        />

        <material id="floorMaterial"
            textureref="tile-floor"
            emissive="0.0 0.0 0.0 1"
            specular="0 0 0 0"
            color="0.8 0.8 0.8 1.0"
            shininess="0"
        />

        <material id="ceilingMaterial"
            textureref="ceiling-texture"
            emissive="0.0 0.0 0.0 1"
            specular="0 0 0 0"
            color="0.8 0.8 0.8 1.0"
            shininess="0"
        />

        <material id="wallMaterial"
            textureref="wall"
            emissive="0.0 0.0 0.0 1"
            specular="0 0 0 0"
            color="0.8 0.8 0.8 1.0"
            shininess="0"
            texlength_t="2"
            texlength_s="4"
        />

        <material id="doorMaterial"
            color="0.9 0.98 0.95 1.0"
            emissive="0.0 0.0 0.0 1"
            specular="0 0 0 0"
            shininess="90"
            textureref="door-texture"
            texlength_s='2'
            texlength_t='4.5'
        />

        <material id="mirrorMaterial"
            textureref="mirror-texture"
            color="1 1 1 1"
            emissive="0.0 0.0 0.0 1"
            specular="0.6 0.6 0.6 1"
            shininess="10"
        />

        <material id="pictureWall1Material"
            textureref="pic1"
            color="1 1 1 1"
            specular="0 0 0 1"
            shininess="0"
            emissive="0 0 0 1"
            texlength_s="1.2"
            texlength_t="1.7"
        />

        <material id="pictureWall2Material"
            textureref="pic2"
            color="1 1 1 1"
            specular="0.01 0.01 0 1"
            shininess="0"
            emissive="0 0 0 1"
            texlength_s="1.2"
            texlength_t="1.7"
        />

        <material id="basketMaterial"
            textureref="basket-texture"
            color="1 1 1 1"
            specular="0 0 0 0"
            shininess="0"
            emissive="0 0 0 0"
            twosided="true"
        />


        <material id="towelsMaterial"
            textureref="towels-texture"
            color="1 1 1 1"
            specular="0 0 0 0"
            shininess="0"
            emissive="0 0 0 0"
            twosided="true"
        />

        <material id="towelsFrontMaterial"
            textureref="towel-front-texture"
            color="1 1 1 1"
            specular="0 0 0 0"
            shininess="0"
            emissive="0 0 0 0"
            twosided="true"
        />

        <material id="carpetMaterial"
            textureref="carpet-texture"
            color="1 1 1 1"
            specular="0 0 0 0"
            shininess="0"
            emissive="0 0 0 0"
            twosided="true"
            bumpref="carpet-bump"
            bumpscale="2"
        />

        <material id="woodMaterial"
            textureref="wood-texture"
            color="1 1 1 1"
            specular="0 0 0 0"
            shininess="0"
            emissive="0 0 0 0"
            twosided="true"
        />

        <material id="videoBathMaterial"
            textureref="video-bath"
            color="1 1 1 1"
            specular="0 0 0 1"
            emissive="0 0 0 1"
            shininess="12"
        />

        <material id="shampooLabelMaterial1"
            textureref="shampoo-label-texture-1"
            color="1 1 1 1"
            specular="0 0 0 0"
            shininess="0"
            emissive="0 0 0 0"
            twosided="true"
        />

        <material id="shampooLabelMaterial2"
            textureref="shampoo-label-texture-2"
            color="1 1 1 1"
            specular="0 0 0 0"
            shininess="0"
            emissive="0 0 0 0"
            twosided="true"
        />

        <material id="glassMaterial"
            color="0.8 0.8 0.8 1"
            specular="0.8 0.8 0.8 1"
            shininess="20"
            emissive="0 0 0 0"
        />

    </materials>

    <graph rootid="scene-real">

        <node id="scene-real" castshadows="true" receiveshadows="true">
            <children>
                <lodref id="scene-lod" />
            </children>
        </node>
        <lod id="scene-lod">
            <noderef id="scene" mindist="0" />
            <noderef id="scene-far" mindist="50" />
        </lod>
        <node id="scene-far">
            <materialref id="farMaterial" />
            <children>
                <noderef id="ikea-thing" />
                <noderef id="garage-door" />
                <primitive>
                    <box xyz1="-5 0 -5" xyz2="5 5 5" />
                </primitive>
            </children>
        </node>
        <node id="garage-door">
            <materialref id="garageMaterial" />
            <children>
                <primitive><box xyz1="-4 0.2 5" xyz2="4 4.5 5.05" /></primitive>
                <primitive><box xyz1="5 0.2 -4" xyz2="5.05 4.5 4" /></primitive>
            </children>
        </node>
        <node id="scene" castshadows="true" receiveshadows="true">
            <children>
                <noderef id="frontWall" />
                <noderef id="backWall" />
                <noderef id="leftWall" />
                <noderef id="rightWall" />
                <noderef id="floor" />
                <noderef id="ceiling" />
                <noderef id="door" />
                <noderef id="mirror-sink-group" />
                <noderef id="picture-wall-1" />
                <noderef id="picture-wall-2" />
                <noderef id="toilet" />
                <noderef id="shelve-group" />
                <noderef id="carpet" />
                <noderef id="basket" />
                <noderef id="shower-group" />
                <noderef id="lods" />

                <pointlight id="pl1" enabled="true"
                    color="0.41 0.37 0.22 1"
                    intensity="50.0"
                    distance="100"
                    decay="2.0"
                    castshadow="true"
                    position="0 4.9 0"
                    shadowfar="100"
                />

                <spotlight id="spl1" enabled="true" color="0.41 0.37 0.22 1" angle="200"
                    position="4.98 4.9 4.98" target="0 0 0" intensity="25" castshadow="true"                     shadowfar="100" />

                <spotlight id="spl2" enabled="true" color="0.41 0.37 0.22 1" angle="200"
                    position="-4.98 4.9 -4.98" target="0 0 0" intensity="25" castshadow="true"                     shadowfar="100" />

                <spotlight id="spl3" enabled="true" color="0.41 0.37 0.22 1" angle="200"
                    position="4.98 4.9 -4.98" target="0 0 0" intensity="25" castshadow="true"                     shadowfar="100" />

                <spotlight id="spl4" enabled="true" color="0.41 0.37 0.22 1" angle="200"
                    position="-4.98 4.9 4.98" target="0 0 0" intensity="25" castshadow="true"                     shadowfar="100"/>

            </children>
        </node>

        <node id="frontWall" castshadows="true" receiveshadows="true">
            <materialref
                id="wallMaterial"
            />
            <transforms>
                <rotate value3="0 -1.57 0" />
                <translate value3="0 2.5 -5.0" />
            </transforms>


            <children>
                <primitive>
                    <rectangle xy1="-5 -2.5" xy2="5 2.5" />
                </primitive>
            </children>
        </node>

        <node id="polygon">
            <children>
                <primitive>
                    <polygon radius="1" stacks="10" slices="10" color_c="0.15 0.20 0.20 1" color_p="0.05 0.05 0.05 1" />
                </primitive>
            </children>
            <transforms>
                <rotate value3="-1.57 0 0" />
                <translate value3="-1.5 1.5 0.07" />
            </transforms>
        </node>

        <node id="backWall" castshadows="true" receiveshadows="true">
            <materialref
                id="wallMaterial"
            />
            <transforms>
                <rotate value3="0 1.57 0.0" />
                <translate value3="0 2.5 -5.0" />
            </transforms>


            <children>
                <primitive>
                    <rectangle xy1="-5 -2.5" xy2="5 2.5" />
                </primitive>
            </children>

        </node>

        <node id="rightWall" castshadows="true" receiveshadows="true">
            <materialref
                id="wallMaterial"
            />
            <transforms>
                <rotate value3="6.28 0.0 0.0" />
                <translate value3="0.0 2.5 -5.0" />
            </transforms>

            <children>
                <primitive>
                    <rectangle xy1="-5 -2.5" xy2="5 2.5" />
                </primitive>
            </children>

        </node>

        <node id="leftWall" castshadows="true" receiveshadows="true">
            <materialref
                id="wallMaterial"
            />
            <transforms>
                <rotate value3="3.14 0.0 3.14" />
                <translate value3="0.0 2.5 -5.0" />
            </transforms>

            <children>
                <primitive>
                    <rectangle xy1="-5 -2.5" xy2="5 2.5" />
                </primitive>
            </children>

        </node>

        <node id="floor" receiveshadows="true">
            <materialref id="black" />
            <transforms>
                <rotate value3="-1.57 0 0" />
            </transforms>

            <children>
                <primitive>
                    <rectangle xy1="-5 -5" xy2="5 5" />
                </primitive>
            </children>

        </node>

        <node id="ceiling" castshadows="true" receiveshadows="true">
            <materialref id="ceilingMaterial" />
            <transforms>
                <translate value3="0 5 0" />
                <rotate value3="1.57 0 0" />
            </transforms>

            <children>
                <primitive>
                    <rectangle xy1="-5 -5" xy2="5 5" />
                </primitive>
            </children>

        </node>


        <node id="door">
            <materialref id="doorMaterial" />
            <children>
                <primitive>
                    <rectangle xy1="0 0" xy2="2 4.5" />
                </primitive>
            </children>
            <transforms>
                <translate value3="-4.9 0 -2.5" />
                <rotate value3="0 1.57 0" />
            </transforms>
        </node>

        <node id="mirror-sink-group">
            <children>
                <noderef id="mirror-group" />
                <noderef id="sink" />
            </children>
            <transforms>
                <translate value3="0.5 0 0" />
            </transforms>
        </node>
        <node id="mirror-group">
            <children>
                <noderef id="mirror" />
            </children>
        </node>
        <node id="mirror" castshadows="true">
            <materialref id="mirrorMaterial" />
            <children>
                <primitive>
                    <cylinder base="1" top="1" height="0.2" slices="30" stacks="20" capsclose="true" />
                </primitive>
                <noderef id="mirror-border" />
            </children>
            <transforms>
                <translate value3="0 3 4.97" />
                <rotate value3="1.57 3.14 0" />
            </transforms>
        </node>
        <node id="mirror-border">
            <materialref id="black" />
            <children>
                <primitive>
                    <cylinder base="1.02" top="1.02" height="0.2" slices="30" stacks="20" />
                </primitive>
            </children>
        </node>
        <node id="picture-wall-1">
            <materialref id="pictureWall1Material" />
            <children>
                <primitive>
                    <rectangle xy1="0 0" xy2="1.2 1.55" />
                </primitive>
            </children>
            <transforms>
                <translate value3="-4.99 2 0" />
                <rotate value3="0 1.57 0" />
            </transforms>
        </node>

        <node id="picture-wall-2">
            <materialref id="pictureWall2Material" />
            <children>
                <primitive>
                    <rectangle xy1="0 0" xy2="1.2 1.55" />
                </primitive>
            </children>
            <transforms>
                <translate value3="-4.99 2.5 2" />
                <rotate value3="0 1.57 0" />
            </transforms>
        </node>


        <node id="toilet">
            <children>
                <noderef id="toilet-bottom-1" />
                <noderef id="toilet-bottom-2" />
                <noderef id="toilet-bottom-3" />
                <noderef id="toilet-bottom-4" />
                <noderef id="toilet-bottom-5" />
                <noderef id="toilet-button" />
            </children>
            <transforms>
                <translate value3="4 0 1" />
                <rotate value3="0 1.57 0" />
            </transforms>
        </node>

        <node id="toilet-bottom-1">
            <materialref id="white" />
            <children>
                <primitive>
                    <cylinder base="0.4" top="0.5" height="0.5" slices="20" stacks="20" />
                </primitive>
            </children>
            <transforms>
                <scale value3="1 2 1.5" />
                <translate value3="0 0.5 0" />
            </transforms>
        </node>

        <node id="toilet-bottom-2">
            <materialref id="white" />
            <children>
                <primitive>
                    <box xyz1="-0.5 0.8 0.4" xyz2="0.5 2 0.7" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 0 0" />
                <scale value3="1 1 1.5" />
            </transforms>
        </node>

        <node id="toilet-bottom-3">
            <materialref id="white" />
            <children>
                <primitive>
                    <sphere radius="0.6" slices="40" stacks="10" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1 0" />
                <scale value3="1 0.4 1.7" />
            </transforms>
        </node>


        <node id="toilet-bottom-4">
            <materialref id="white" />
            <children>
                <primitive>
                    <sphere radius="0.7" slices="40" stacks="10" thetalength="1.57" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1 0" />
                <scale value3="0.8 -1 1.3" />
            </transforms>
        </node>

        <node id="toilet-bottom-5">
            <materialref id="white" />
            <children>
                <primitive>
                    <cylinder base="0.3" top="0.3" height="1" slices="10" stacks="10" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 0.5 0.5" />
                <scale value3="1 1 1.8" />
            </transforms>
        </node>

        <node id="toilet-button">
            <materialref id="grey-metal" />
            <children>
                <primitive>
                    <sphere radius="0.1" slices="10" stacks="10" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 2 0.8" />
                <scale value3="1 0.5 1" />
            </transforms>
        </node>


        <node id="sink" castshadows="true" receiveshadows="true">
            <materialref id="white" />
            <children>
                <noderef id="sink-base-wood" />
                <noderef id="sink-actual-base" />
                <noderef id="sink-handler" />

            </children>
            <transforms>
                <translate value3="0 0.7 4.3" />
            </transforms>
        </node>

        <node id="sink-base-wood">
            <materialref id="black" />
            <children>
                <noderef id="sink-front-shelve" />
                <primitive>
                    <box xyz1="-1.2 0 -0.7" xyz2="1.2 1 0.7" />
                </primitive>
            </children>
        </node>
        <node id="sink-front-shelve">
            <materialref id="woodMaterial" />
            <children>
                <primitive>
                    <box xyz1="-1.2 0 -0.7" xyz2="1.2 1 -0.75" />
                </primitive>
            </children>
        </node>

        <node id="sink-actual-base">
            <children>
                <noderef id="sink-actual-base-bottom" />
                <noderef id="sink-actual-base-left" />
                <noderef id="sink-actual-base-right" />
                <noderef id="sink-actual-base-front" />
                <noderef id="sink-actual-base-back" />
                <noderef id="sink-button" />
            </children>
        </node>

        <node id="sink-actual-base-bottom">
            <children>
                <primitive>
                    <box xyz1="-0.6 0 -0.5" xyz2="0.6 0.1 0.2" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1 0" />
            </transforms>
        </node>

        <node id="sink-actual-base-left">
            <children>
                <primitive>
                    <box xyz1="-0.6 0 -0.4" xyz2="-0.5 0.2 0.2" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1 0" />
            </transforms>
        </node>

        <node id="sink-actual-base-right">
            <children>
                <primitive>
                    <box xyz1="0.6 0 -0.4" xyz2="0.5 0.2 0.2" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1 0" />
            </transforms>
        </node>

        <node id="sink-actual-base-front">
            <children>
                <primitive>
                    <box xyz1="-0.6 0 -0.5" xyz2="0.6 0.2 -0.4" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1 0" />
            </transforms>
        </node>

        <node id="sink-actual-base-back">
            <children>
                <primitive>
                    <box xyz1="-0.6 0 0.3" xyz2="0.6 0.2 0.2" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1 0" />
            </transforms>
        </node>

        <node id="sink-button">
            <materialref id="grey-metal" />
            <children>
                <primitive>
                    <sphere radius="0.1" slices="10" stacks="10" thetalength="1.57" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1.05 0" />
            </transforms>
        </node>

        <node id="sink-handler">
            <materialref id="grey-metal" />
            <children>
                <noderef id="sink-handler-1" />
                <noderef id="sink-handler-2" />
                <noderef id="sink-handler-3" />
            </children>
        </node>
        <node id="sink-handler-1">
            <children>
                <primitive>
                    <cylinder base="0.08" top="0.08" height="0.8" slices="10" stacks="3" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1.3 0.5" />
            </transforms>

        </node>
        <node id="sink-handler-2">
            <children>
                <primitive>
                    <cylinder base="0.04" top="0.04" height="0.4" slices="10" stacks="3" />
                </primitive>
            </children>
            <transforms>
                <translate value3="0 1.5 0.35" />
                <rotate value3="1.047 0 0" />
            </transforms>

        </node>

        <node id="sink-handler-3">
            <children>
                <primitive>
                    <box xyz1="-0.08 0 -0.15" xyz2="0.08 0.03 0.25" />
